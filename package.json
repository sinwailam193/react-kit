{
  "name": "onNez",
  "version": "1.0.0",
  "description": "Frontend side of our App",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf buildServer.js public",
    "build-server": "webpack --config build/webpack.server.js",
    "dev": "npm run clean && npm run build-server && npm-run-all --parallel dev:*",
    "dev:build": "webpack --config build/webpack.client.js --watch",
    "dev:server": "node buildServer.js",
    "heroku-prebuild": "npm install --only=dev",
    "prestart": "npm run clean && npm run build-server && NODE_ENV=production webpack --config build/webpack.client.js",
    "start": "NODE_ENV=production node buildServer.js"
  },
